{
  "name": "roytracker",
  "version": "1.1.61",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "expo build",
    "prebuild": "npm run type-check",
    "version:patch": "npm install && git add . && git commit -m \"chore: update\" && npm version patch -m \"chore(release): bump patch to %s\" && git push --follow-tags",
    "version:minor": "npm install && git add . && git commit -m \"chore: update\" && npm version minor -m \"chore(release): bump minor to %s\" && git push --follow-tags",
    "version:major": "npm install && git add . && git commit -m \"chore: update\" && npm version major -m \"chore(release): bump major to %s\" && git push --follow-tags",
    "auto:patch-build": "ts-node scripts/autoPatchBuild.cts",
    "build:apk-local": "ts-node scripts/localApkBuild.cts"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-navigation/bottom-tabs": "^7.8.8",
    "@react-navigation/native": "^7.1.22",
    "expo": "^54.0.29",
    "expo-crypto": "^15.0.8",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "^19.0.21",
    "expo-linear-gradient": "^15.0.8",
    "expo-local-authentication": "^17.0.8",
    "expo-navigation-bar": "~5.0.10",
    "expo-sharing": "^14.0.8",
    "expo-status-bar": "^3.0.9",
    "expo-system-ui": "^6.0.9",
    "lodash": "^4.17.21",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gifted-charts": "^1.4.69",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "victory-native": "^36.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@types/lodash": "^4.17.21",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.7",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
